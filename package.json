{
  "name": "react-immer",
  "version": "1.3.0",
  "description": "No nonsense state management with Immer and React hooks",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monojack/react-immer.git"
  },
  "keywords": [
    "javascript",
    "react",
    "hooks",
    "immer",
    "state"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "scripts": {
    "dev": "npm run build:esm -- -w",
    "pretest": "npm run build:cjs",
    "test": "ava",
    "build:esm": "BABEL_ENV=esm babel src --out-dir esm",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir lib",
    "build": "npm run build:cjs && npm run build:esm",
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf lib esm"
  },
  "ava": {
    "files": [
      "test/*.js",
      "!test/mocks/**"
    ]
  },
  "bugs": {
    "url": "https://github.com/monojack/react-immer/issues"
  },
  "npmName": "react-immer",
  "files": [
    "esm",
    "lib"
  ],
  "author": "Ionut Achim <achimvionut@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "dependencies": {
    "symbol-observable": "1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "^7.0.0",
    "ava": "^0.25.0",
    "babel-eslint": "^10.0.1",
    "eslint": "5.6.1",
    "eslint-config-mono": "2.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "immer": "1.8.0",
    "react": "^16.7.0-alpha.2",
    "rimraf": "2.6.2"
  },
  "peerDependencies": {
    "react": "^16.7.*",
    "immer": "^1.8.*"
  }
}
